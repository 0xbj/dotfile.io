BODY {
	font-size:	12px;
	font-family:	Arial,Verdana,sans;
	background:	#56565a;
}

a {
	color:		#222227;
}

ul {
	list-style:	none;
}

.ulbull {
	list-style:	disc;
}

.subli {
	padding-left:10px;

}

#topframe {
	position:	absolute;
	top:			0px;
	left:			0px;
	width:		100%;
	height:		100px;
	background:	#232326;
	text-align:	center;
}

#topstripe {
	position:	absolute;
	top:			100px;
	left:			0px;
	width:		100%;
	height:		16px;
	background:	#919196;
	text-align:	right;

}

#backlink {
	color:		#232326;
	margin-right:	20px;
	font-weight:	bold;
}

#title {
	color:		#919196;
	font-weight:	bold;
	font-size:		30px;
}

#content {
	position:	absolute;
	left:			20px;
	top:			140px;
	width: 		600px;
}

.embedimg {
	margin-top:	30px;
}

.embedimg p {
	font-style:	italic;
	color:		#232326;
}